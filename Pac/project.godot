config_version=5

[application]

config/name="Pactroidvania"
run/main_scene="res://the_maze.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://7-76276_games-pacman-png-transparent-png.png"

[display]

window/size/viewport_width=240
window/size/viewport_height=320
window/size/resizable=false
window/size/window_width_override=720
window/size/window_height_override=960
window/stretch/mode="viewport"

[global_group]

tilemap=""

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
